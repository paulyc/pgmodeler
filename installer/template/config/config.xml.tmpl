<?xml version="1.0" encoding="UTF-8"?>
<Installer>
    <Name>PostgreSQL Database Modeler</Name>
    <Version>{version}</Version>
    <Title>pgModeler</Title>
    <Publisher>Raphael Ara√∫jo e Silva (raphael@pgmodeler.io)</Publisher>
    <TargetDir>{prefix}</TargetDir>
    <InstallerApplicationIcon>installer_icon</InstallerApplicationIcon>
    <InstallerWindowIcon>installer_icon</InstallerWindowIcon>
    <Logo>installer_logo.png</Logo>
    <Watermark>watermark.png</Watermark>
    <RemoveTargetDir>true</RemoveTargetDir>
    <AllowSpaceInPath>true</AllowSpaceInPath>
	<WizardDefaultHeight>450</WizardDefaultHeight>
    <WizardDefaultWidth>500</WizardDefaultWidth>
	<WizardStyle>Classic</WizardStyle>
	<SupportsModify>True</SupportsModify>
	<MaintenanceToolName>uninstall</MaintenanceToolName>
	<StartMenuDir>pgModeler</StartMenuDir>
    <ControlScript>controlscript.qs</ControlScript>
</Installer>
